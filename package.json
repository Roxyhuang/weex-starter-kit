{
  "name": "weex-starter-kit",
  "description": "weex-starter-kit",
  "version": "0.1.0",
  "author": "Neo Huang",
  "private": true,
  "main": "index.js",
  "keywords": [
    "weex-starter-kit",
    "weex",
    "vue"
  ],
  "scripts": {
    "dev": "npm run local",
    "local": "node build/dev-server.js --local",
    "build:dev": "node build/build.js --dev",
    "build:qa": "node build/build.js --qa",
    "build:prod": "node build/build.js --prod",
    "serve": "node mock/api-server.js && serve -p 8081",
    "debug": "weex-devtool",
    "eslint": "eslint --ext .js --ext .vue src webpack.config.js",
    "clean": "rm -rf dist && mkdir -p dist",
    "copy": "cp -R assets/img dist/img && cp -R assets/style.css dist/style.css && cp ./node_modules/vue/dist/vue.js dist/vue.js && cp ./node_modules/weex-vue-render/dist/index.js dist/weex-vue-render.js"
  },
  "dependencies": {
    "vue": "^2.1.8",
    "vue-router": "^2.1.1",
    "vuex": "^2.1.1",
    "vuex-router-sync": "^4.0.1",
    "weex-vue-framework": "^2.2.1-weex.1",
    "weex-vue-render": "^0.11.41"
  },
  "devDependencies": {
    "assets-webpack-plugin": "^3.5.1",
    "babel-core": "^6.20.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.9",
    "babel-plugin-transform-async-to-generator": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-polyfill": "^6.22.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-0": "^6.0.15",
    "babel-runtime": "^6.11.6",
    "connect-history-api-fallback": "^1.3.0",
    "css-loader": "^0.26.1",
    "eslint": "^3.13.1",
    "eslint-config-airbnb-base": "^11.0.1",
    "eslint-config-vue": "^2.0.1",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-html": "^1.7.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-vue": "^1.0.0",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "fake": "^0.2.2",
    "faker": "^4.1.0",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.28.0",
    "nib": "^1.1.2",
    "ora": "^0.2.0",
    "serve": "^1.4.0",
    "style-loader": "^0.13.1",
    "stylus": "^0.54.5",
    "stylus-px2rem": "^1.0.11",
    "url-loader": "^0.5.7",
    "vue-loader": "^10.0.2",
    "vue-template-compiler": "^2.1.7",
    "webpack": "^1.14.0",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.6.0",
    "webpack-merge": "^4.1.0",
    "weex-devtool": "^0.2.77",
    "weex-loader": "^0.4.4",
    "weex-vue-loader": "^0.2.11"
  }
}
